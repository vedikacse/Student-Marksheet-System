#include <stdio.h>
int main ()
{
int marks;
printf("Enter your marks");
scanf("%d",&marks);

if (marks<30)
{printf("you get c grade\n");
}
else if (30<=marks && 70>=marks)
{ printf("you get B grade");}
else if (70<=marks && marks<=90)
{printf("you get A grade");}
else if (90<=marks && marks<=100)
{printf("you get A+ grade");
}else
{printf("you are failed");}
printf("Thank youðŸ™");
return 0;}
